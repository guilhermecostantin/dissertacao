@MASTERSTHESIS{Spiegl2007,
  author = {Spiegl, Klaus},
  title = {{Projektmanagement life - Best Practices und Significant Events im
	Software Projektmanagement}},
  year = {2007},
  abstract = {Noch immer gelingt es einem Gro\{\ss\}teil der Software-Entwicklungsprojekte
	nicht, die geforderten Funktionalit\{\"{a}\}ten in der vorgegeben
	Zeit mit den geplanten Ressourcen umzusetzen. In den letzten Jahren
	entstanden in vielen Bereichen des Projektmanagements immer mehr
	Methoden und Entwicklungsphilosophien, welche einen "Pool" von "Best
	Practices" verwenden. Dieser Methodenpool sollte den Projektmanagern
	die Arbeit erleichtern und die Erfolgsquote der Projekte verbessern.
	Was bedeuten "Best Practices" f\{\"{u}\}r Projektleiter und den Projekterfolg
	jedoch wirklich?
	
	Mit dieser Arbeit wurden zwei Ziele verfolgt. Das erste Ziel war das
	"Nebeneinander-Stellen" des theoretischen Projektmanagements (Literatur)
	und des Projektmanagementalltags (Interviews). Das zweite Ziel war
	die Ausarbeitung und Analyse von "Best Practices" und "Significant
	Events" im Rahmen der Experteninterviews. Die Daten wurden mittels
	halbstrukturierten Interviews mit erfahrenen Projektleitern und CIOs
	zu den Themen "Best Practices" und "Significant Events" erhoben.
	
	
	Die Interviewdaten wurden in folgende Richtungen ausgewertet: Welche
	Erfahrungen haben erfahrene Projektleiter und CIOs in ihrer Karrierelaufbahn
	gemacht? Wo "zwickt es" im t\{\"{a}\}glichen Projektgesch\{\"{a}\}ft?
	Was sind immer wiederkehrende Probleme und was sind die ganz pers\{\"{o}\}nlichen
	L\{\"{o}\}sungs- und Erfolgsstrategien der Entscheidungstr\{\"{a}\}ger
	f\{\"{u}\}r diese Probleme. Es wurden die allt\{\"{a}\}glichen Probleme,
	deren Ursachen und Auswirkungen aufzeigt, mit welchen Projektmanager
	im Rahmen eines Softwareprojektes in der Praxis regelm\{\"{a}\}\{\ss\}ig
	konfrontiert sind.
	
	
	Weiters wurden signifikante Ereignisse aus abgeschlossenen Projekten
	n\{\"{a}\}her betrachtet. Dabei wurden Antworten auf die Fragen "Wie
	kam es zu diesem Ereignis?", "Welche Personen (Rollen) spielten eine
	wesentliche Rolle?", "Wie h\{\"{a}\}tte man diese Situation verhindern
	k\{\"{o}\}nnen bzw. wie kann man gezielt auf diese Situation "hinarbeiten"?",
	"Was kann man dadurch allgemein f\{\"{u}\}r (zuk\{\"{u}\}nftige)
	Software-Projekte ableiten?" und "Was f\{\"{u}\}r Auswirkungen hatte
	das Ereignis auf den Projektverlauf?" gesucht. Die daf\{\"{u}\}r
	aus den verschiedenen Einzelinterviews erhobenen signifikanten Ereignisse
	wurden miteinander verglichen, um \{\"{A}\}hnlichkeiten herauszufinden.},
  publisher = {Universit\{\"{a}\}t Wien \& Technische Universit\{\"{a}\}t Wien},
  url = {http://eprints.cs.univie.ac.at/467/}
}

